{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "sis-ui",
  "version": "0.1.2",
        "identifier": "lorinta.sis-ui",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
  "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
  "targets": ["deb", "rpm"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
